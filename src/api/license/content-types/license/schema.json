{
  "kind": "collectionType",
  "collectionName": "licenses",
  "info": {
    "singularName": "license",
    "pluralName": "licenses",
    "displayName": "License",
    "description": "Business licenses and credentials management"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "type": {
      "type": "string",
      "maxLength": 255,
      "required": true
    },
    "number": {
      "type": "string",
      "maxLength": 255,
      "required": true
    },
    "category": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "license",
        "certification",
        "business",
        "insurance"
      ]
    },
    "license_status": {
      "type": "enumeration",
      "required": true,
      "default": "active",
      "enum": [
        "active",
        "inactive",
        "pending",
        "expired"
      ]
    },
    "isPublic": {
      "type": "boolean",
      "required": true,
      "default": true
    },
    "description": {
      "type": "text"
    },
    "issuingAuthority": {
      "type": "string",
      "maxLength": 255
    },
    "expirationDate": {
      "type": "date"
    }
  }
}
