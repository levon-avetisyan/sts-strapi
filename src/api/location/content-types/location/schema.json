{
  "kind": "collectionType",
  "collectionName": "locations",
  "info": {
    "singularName": "location",
    "pluralName": "locations",
    "displayName": "Location"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "required": true
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "comingSoon": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "servicedCities": {
      "type": "json"
    },
    "stateCode": {
      "type": "string",
      "required": true
    },
    "phoneNumber": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email"
    },
    "image": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "seo": {
      "type": "component",
      "component": "seo.seo",
      "repeatable": true
    }
  }
}
